{
    "name": "salmanzafar/laravel-mqtt",
    "description": "A simple MQTT Library to connect/publish/subscribe to a MQTT broker using Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Salman Zafar",
            "email": "salmanzafar949@gmail.com"
        }
    ],
    "require": {
        "php": ">=7"
    },
    "require-dev": {
        "laravel/framework": ">=5.5"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Salman\\Mqtt\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Salman\\Mqtt\\MqttServiceProvider"
            ],
            "aliases": {
                "Mqtt": "Salman\\Mqtt\\Facades\\Mqtt"
            }
        }
    }
}
